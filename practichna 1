#include <iostream>
using namespace std;

int main() {
    int w, h;
    char ch;
    cout << "Ширина, висота, символ: ";
    cin >> w >> h >> ch;

    cout << "1 - повний, 2 - порожній: ";
    int type;
    cin >> type;

    for (int i = 0; i < h; i++) {
        for (int j = 0; j < w; j++) {
            if (type == 2 && i > 0 && i < h - 1 && j > 0 && j < w - 1)
                cout << " ";
            else
                cout << ch;
        }
        cout << "\n";
    }
}
