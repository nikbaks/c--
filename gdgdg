#include <iostream>
using namespace std;

void rectangle(int N, int K) {
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < K; j++)
            cout << "*";
        cout << endl;
    }
}

int factorial(int n) {
    int f = 1;
    for (int i = 1; i <= n; i++)
        f *= i;
    return f;
}

bool isPrime(int n) {
    if (n < 2) return false;
    for (int i = 2; i * i <= n; i++)
        if (n % i == 0) return false;
    return true;
}

int cube(int x) {
    return x * x * x;
}

int maxNum(int a, int b) {
    return (a > b) ? a : b;
}

bool isPositive(int x) {
    return x > 0;
}

int main() {
    int n, k;
    cout << "ведіть висоту і ширину прямокутника: ";
    cin >> n >> k;
    rectangle(n, k);

    cout << "\nведіть число для факторіала: ";
    cin >> n;
    cout << "Факторіал = " << factorial(n) << endl;

    cout << "\nведіть число для перевірки на просте: ";
    cin >> n;
    cout << (isPrime(n) ? "Просте число" : "Не просте число") << endl;

    cout << "\nведіть число для куба: ";
    cin >> n;
    cout << "Куб = " << cube(n) << endl;

    int a, b;
    cout << "\nведіть два числа: ";
    cin >> a >> b;
    cout << "Найбільше = " << maxNum(a, b) << endl;

    cout << "\nведіть число для перевірки на додатність: ";
    cin >> n;
    cout << (isPositive(n) ? "Додатне" : "Від’ємне або нуль") << endl;

    return 0;
}
