#include <iostream>
using namespace std;

bool isLeap(int year) {
    return (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);
}

int daysInMonth(int month, int year) {
    int days[] = { 31,28,31,30,31,30,31,31,30,31,30,31 };
    if (month == 2 && isLeap(year)) return 29;
    return days[month - 1];
}

int daysFromStart(int d, int m, int y) {
    int total = 0;
    for (int i = 1; i < y; i++)
        total += isLeap(i) ? 366 : 365;
    for (int i = 1; i < m; i++)
        total += daysInMonth(i, y);
    total += d;
    return total;
}

int diffDays(int d1, int m1, int y1, int d2, int m2, int y2) {
    return abs(daysFromStart(d1, m1, y1) - daysFromStart(d2, m2, y2));
}

int main() {
    cout << diffDays(1, 1, 2024, 1, 1, 2025);
}
